<header class="main-heading" data-turbolinks='false'>
  <div class="container-fluid">
    <div class="row">
      <div class="col-xl-8 col-lg-8 col-md-8 col-sm-8">
        <div class="page-icon">
          <i class="icon-people"></i>
        </div>
        <div class="page-title">
          <h5>Member Details</h5>
        </div>
      </div>
    </div>
  </div>
</header>
<div class="main-content" data-turbolinks='false'>
  <div class="row gutters">
    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">
        <div class="card">
          <div class="card-body table-responsive table-width1">
            <div class="row">
              <div class="col-md-4 col-xs-12">
                <div class="form-group">
                   <h5>Name: <label for="" style="margin-left:50px"><%= @member.name %></label></h5>
                </div>
              </div>

              <div class="col-md-12 col-xs-12">
                <div class="form-group">
                  <%= link_to 'Edit', edit_member_path(@member), class: 'btn btn-primary' %>
                </div>
              </div>
            </div>
          </div>
        </div>
    </div>
  </div>
</div>

